{
  "name": "rsk-dividend-service",
  "script": "./dist/main.js",
  "watch": false,
  "env_dev": {
    "NODE_ENV": "dev"
  },
  "env_test": {
    "NODE_ENV": "test"
  },
  "env_uat": {
    "NODE_ENV": "uat"
  },
  "env_prod": {
    "NODE_ENV": "prod"
  },
  "merge_logs": true,
  "log_date_format": "YYYY-MM-DD HH:mm:ss",
  "min_uptime": "60s",
  "max_restarts": 30,
  "autorestart": true,
  "restart_delay": 60
}
